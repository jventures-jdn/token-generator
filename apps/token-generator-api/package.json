{
  "name": "@jventures-jdn/token-generator-api",
  "version": "0.1.0",
  "author": "J Ventures Co., Ltd.",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^5.1.1",
    "@nomicfoundation/hardhat-verify": "^2.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "event-stream": "^4.0.1",
    "fs-extra": "^11.2.0",
    "hardhat": "^2.19.2",
    "helmet": "^7.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@jventures-jdn/config-chains": "*",
    "@jventures-jdn/config-consts": "*",
    "@jventures-jdn/config-prettier": "*",
    "@jventures-jdn/config-tsconfig": "*",
    "@jventures-jdn/eslint-config-nest": "*",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@openzeppelin/contracts": "^4.9.0",
    "@typechain/ethers-v5": "^11.1.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "copyfiles": "^2.4.1",
    "ethers": "^5.7.2",
    "jest": "^29.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "nest build && pnpm build:copy",
    "build:copy": "copyfiles ./contracts/**/*.sol dist && copyfiles ./package.json dist && copyfiles ./hardhat.config.ts dist",
    "clean": "rm -rf node_modules && rm -rf dist",
    "dev": "export NODE_ENV=development && nest start --watch",
    "hardhat:compile": "pnpm hardhat compile",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "start": "node dist/main",
    "start:debug": "nest start --debug --watch",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  }
}
