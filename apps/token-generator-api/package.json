{
  "name": "@jventures-jdn/token-generator-api",
  "version": "1.2.0",
  "author": "J Ventures Co., Ltd.",
  "dependencies": {
    "@jventures-jdn/api-fetcher": "workspace:*",
    "@jventures-jdn/config-chains": "workspace:*",
    "@jventures-jdn/config-consts": "*",
    "@jventures-jdn/tools": "*",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^5.1.1",
    "@nomicfoundation/hardhat-verify": "^2.0.3",
    "bull": "^4.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "event-stream": "^4.0.1",
    "fs-extra": "^11.2.0",
    "hardhat": "^2.19.2",
    "helmet": "^7.1.0",
    "json-bigint": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@jventures-jdn/config-prettier": "*",
    "@jventures-jdn/config-tsconfig": "*",
    "@jventures-jdn/eslint-config-nest": "*",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts": "^4.9.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/json-bigint": "^1.0.4",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "copyfiles": "^2.4.1",
    "ethers": "5.7.2",
    "hardhat-jest": "^1.0.8",
    "jest": "^29.5.0",
    "redis-memory-server": "^0.10.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "viem": "^1.19.10",
    "webpack-node-externals": "^3.0.0"
  },
  "license": "MIT",
  "private": true,
  "redisMemoryServer": {
    "disablePostinstall": "1"
  },
  "scripts": {
    "build": "nest build && pnpm build:copy",
    "build:copy": "copyfiles ./contracts/**/*.sol dist && copyfiles ./package.json dist && copyfiles ./hardhat.config.ts dist",
    "clean": "rm -rf node_modules && rm -rf dist",
    "dev": "export NODE_ENV=development && nest start --watch",
    "hardhat:compile": "pnpm hardhat compile",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "start": "node dist/main",
    "start:debug": "nest start --debug --watch",
    "test": "pnpm test:unit & pnpm test:contract & pnpm test:e2e",
    "test:contract": "pnpm hardhat jest test/solidity/**/*.spec.ts",
    "test:cov": "pnpm test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --verbose --passWithNoTests --detectOpenHandles --forceExit ./test/integration/.*.spec\\.ts$",
    "test:e2e:cov": "pnpm test:e2e -- --coverage",
    "test:unit": "jest --verbose --passWithNoTests --detectOpenHandles --forceExit ./test/unit/.*.spec\\.ts$",
    "test:watch": "jest --watch"
  }
}
