{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "NestJs",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "ESNext",
    "sourceMap": true,
    "incremental": true,
    "declaration": false,
    "declarationMap": false,
    "allowJs": true
  }
}
